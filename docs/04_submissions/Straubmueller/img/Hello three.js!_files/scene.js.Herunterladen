import * as THREE from "/node_modules/.vite/deps/three.js?v=b2e3f559";

			import Stats from "/node_modules/.vite/deps/three_addons_libs_stats__module__js.js?v=cefa90b0";

			import { OrbitControls } from "/node_modules/.vite/deps/three_addons_controls_OrbitControls__js.js?v=f5140dbf";
			import {FBXLoader} from "/node_modules/.vite/deps/three_addons_loaders_FBXLoader__js.js?v=61a8f456";
			import { GUI } from "/node_modules/.vite/deps/three_addons_libs_lil-gui__module__min__js.js?v=8b95cbac";


// SCENE
const scene = new THREE.Scene();
scene.fog = new THREE.FogExp2(0xa0a0a0, 0.002);


//LIGHTING
const hemiLight = new THREE.HemisphereLight( 0xffffff, 0x444444, 5 );
				hemiLight.position.set( 0, 200, 0 );
				scene.add( hemiLight );

				const dirLight = new THREE.DirectionalLight( 0xffffff, 5 );
				dirLight.position.set( 0, 200, 100 );
				dirLight.castShadow = true;
				dirLight.shadow.camera.top = 180;
				dirLight.shadow.camera.bottom = - 100;
				dirLight.shadow.camera.left = - 120;
				dirLight.shadow.camera.right = 120;
				scene.add( dirLight );


const mesh = new THREE.Mesh( new THREE.PlaneGeometry( 500, 500 ), new THREE.MeshPhongMaterial( { color: 0x999999, depthWrite: false } ) );
mesh.rotation.x = - Math.PI / 2;
mesh.receiveShadow = true;
scene.add( mesh );



// CAMERA
const fov = 50;
const aspect = window.innerWidth / window.innerHeight;
const near = 0.1;
const far = 1000;
const camera = new THREE.PerspectiveCamera(fov, aspect, near, far);
camera.position.set(2, .5, 3);
camera.lookAt(0,1000,0);


// RENDERER
const canvas = document.querySelector("#canvasThree");
const renderer = new THREE.WebGLRenderer({canvas, antialias: true});
renderer.setSize( window.innerWidth, window.innerHeight);

const controls = new OrbitControls(camera, renderer.domElement);
controls.update();

// GEOMETRY

const loader = new FBXLoader();

loader.load('Banana_Mesh.fbx', (fbx) => {
fbx.scale.set(0.01,0.01,0.01);
scene.add(fbx);
});


function animate() {
  requestAnimationFrame(animate);
  controls.update();
  renderer.render(scene, camera);
}
animate();


